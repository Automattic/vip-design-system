{
  "name": "vip-valet-tokens",
  "version": "1.0.0",
  "description": "VIP Valet Figma design token ingest. Produces theme.json and component styling.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node dist/ingest-tokens.js",
    "build": "tsc --project .",
    "build:watch": "tsc --project . -w",
    "run:watch": "nodemon --watch dist/ --ignore \"*.json\" dist/ingest-tokens.js",
    "dev": "concurrently -k \"npm run build:watch\" \"npm run run:watch\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.3.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.18"
  }
}
